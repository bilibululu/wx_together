page{
  background-color: #f7f7f7;
}

.btn {
  width: 80%;
  height: 130rpx;
  margin-top: 60rpx;
  background-color:rgb(48, 133, 126);
  color: white;
  border-radius: 98rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.btn::after {
  border-radius: 98rpx;
  border: 0; 
  margin-top: 0rpx;
}

.textarea{
  width:90%;
  
  margin-top: 30rpx;
  margin-left: 5%;
  margin-right: 5%;
  background-color: #fff;
}

.textinput{
  width:96%;
  height:300rpx;
  margin-left: 2%;
  margin-right: 2%;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  font-size: 34rpx;
}

.click{
  margin-top: 10rpx;
}
.label{
  display: flex;
  margin-top: 20rpx;
}
