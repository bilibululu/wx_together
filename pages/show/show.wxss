page {
  background-color: #efefef;
}

.scr {
  background-color: #efefef;
  width: 100%;
  height: 91%;
  left: 0rpx;
  right: 0rpx;
  top: 0rpx;
  position: absolute;
}

.btnapply {
  font-size: 35rpx;
margin-right: 15rpx;
color: white;
background-color: #5cbcaf;
  border-radius: 98rpx;
  padding: 2rpx 40rpx;
  margin-block-end: 15rpx;
  display: flex;
  align-items: center;
  flex-direction: row;
  justify-content: space-between;
}

.block {
  margin: 15rpx;
  display: flex;
  align-items: flex-end;
  flex-direction: row;
  justify-content: space-between;
}

.al_apply {
  font-size: 30rpx;
  color: #5cbcaf;
  height: 32rpx;
  margin-left: 15rpx;
  margin-block-end: 15rpx;
  display: flex;
  align-items: flex-end;
  flex-direction: row;
  justify-content: space-between;
}
